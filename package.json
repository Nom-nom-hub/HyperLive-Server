    "menus": {
      "explorer/context": [
        {
          "command": "advancedLiveServer.openCurrentFile",
          "group": "navigation",
          "when": "resourceExtname =~ /\\.(html|htm|css|js|jsx|ts|tsx|vue|svelte)$/i"
        }
      ],
      "editor/context": [
        {
          "command": "advancedLiveServer.openCurrentFile",
          "group": "navigation",
          "when": "resourceExtname =~ /\\.(html|htm|css|js|jsx|ts|tsx|vue|svelte)$/i"
        }
      ]
    },
    "configuration": {
      "title": "Advanced Live Server",
      "properties": {
        "advancedLiveServer.port": {
          "type": "number",
          "default": 5500,
          "description": "Port to run the server on"
        },
        "advancedLiveServer.https": {
          "type": "boolean",
          "default": false,
          "description": "Enable HTTPS with self-signed certificates"
        },
        "advancedLiveServer.spa": {
          "type": "boolean",
          "default": false,
          "description": "Enable SPA mode with history API fallback"
        },
        "advancedLiveServer.openBrowser": {
          "type": "boolean",
          "default": true,
          "description": "Automatically open browser when server starts"
        },
        "advancedLiveServer.showOverlay": {
          "type": "boolean",
          "default": true,
          "description": "Show developer tools overlay in browser"
        },
        "advancedLiveServer.watchPatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/*.html",
            "**/*.css",
            "**/*.js"
          ],
          "description": "File patterns to watch for changes"
        },
        "advancedLiveServer.ignorePatterns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "**/node_modules/**",
            "**/.git/**"
          ],
          "description": "File patterns to ignore"
        },
        "advancedLiveServer.ngrokAuthToken": {
          "type": "string",
          "default": "",
          "description": "ngrok authentication token for cloud preview (optional)"
        },
        "advancedLiveServer.enableCloudPreview": {
          "type": "boolean",
          "default": false,
          "description": "Enable cloud preview with ngrok tunneling"
        },
        "advancedLiveServer.proxy": {
          "type": "object",
          "default": {},
          "description": "API proxy rules mapping paths to target URLs"
        },
        "advancedLiveServer.aiMode": {
          "type": "string",
          "default": "local",
          "enum": [
            "local",
            "cloud",
            "hybrid"
          ],
          "description": "AI mode: local (fast, free, offline), cloud (powerful, requires API), hybrid (best of both)"
        },
        "advancedLiveServer.aiProvider": {
          "type": "string",
          "default": "openai",
          "description": "AI model provider (openai, anthropic, ollama, local, gemini, cohere, mistral, perplexity, together, huggingface, openrouter, groq, deepseek, fireworks, nomic, custom)"
        },
        "advancedLiveServer.aiModel": {
          "type": "string",
          "default": "gpt-4",
          "description": "AI model name (e.g., gpt-4, claude-3, etc.)"
        },
        "advancedLiveServer.aiApiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the selected AI provider"
        },
        "advancedLiveServer.aiBaseUrl": {
          "type": "string",
          "default": "",
          "description": "Custom base URL for self-hosted or local AI models"
        },
        "advancedLiveServer.aiTemperature": {
          "type": "number",
          "default": 0.7,
          "description": "AI response creativity (temperature)"
        },
        "advancedLiveServer.aiMaxTokens": {
          "type": "number",
          "default": 2048,
          "description": "Maximum tokens for AI responses"
        },
        "advancedLiveServer.aiEnableErrorExplanation": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered error explanation"
        },
        "advancedLiveServer.aiEnableCodeSuggestions": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered code suggestions"
        },
        "advancedLiveServer.aiEnableAccessibilityAnalysis": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI-powered accessibility analysis"
        }
      }
    }
